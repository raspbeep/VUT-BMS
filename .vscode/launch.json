{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/rds_encoder",
      "args": [
        "-g",
        "0A",
        "-pi",
        "4660",
        "-pty",
        "5",
        "-tp",
        "1",
        "-ms",
        "0",
        "-ta",
        "1",
        "-af",
        "104.5,107.9",
        "-ps",
        "\"Radio\""
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "Python Debugger: Python File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}"
    }
  ]
}